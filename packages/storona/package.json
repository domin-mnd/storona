{
  "name": "storona",
  "version": "1.1.0",
  "description": "Simple opinionated file-based routing for your JavaScript applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "homepage": "https://storona.domin.lol",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./adapter": {
      "import": "./dist/adapter.mjs",
      "types": "./dist/adapter.d.ts",
      "require": "./dist/adapter.js"
    }
  },
  "repository": {
    "url": "https://github.com/domin-mnd/storona.git",
    "type": "git",
    "directory": "packages/storona"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "publish:test": "npm publish --dry-run"
  },
  "keywords": [
    "express",
    "fastify",
    "grammy",
    "backend",
    "router",
    "routing"
  ],
  "author": "domin-mnd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/domin-mnd/storona/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "tsup": "8.3.5"
  },
  "devDependencies": {
    "@domin-mnd/config": "0.0.3",
    "@types/node": "^18",
    "ansis": "3.3.2",
    "terser": "5.37.0",
    "tsup": "8.3.5",
    "typescript": "5.4.4",
    "vitest": "2.0.5"
  }
}
